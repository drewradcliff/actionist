{
	"expo": {
		"name": "Actionist",
		"slug": "actionist",
		"version": "1.4.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "actionist",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.sirampr.actionist"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "a841d4fb-7d96-413b-b5f3-9b157705fb42"
			}
		},
		"owner": "sirampr",
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/a841d4fb-7d96-413b-b5f3-9b157705fb42"
		}
	}
}
